{
	"format_version": "1.8.0",
	"animations": {
		"animation.billelectriceel.swim": {
			"loop": true,
			"anim_time_update": "math.max(query.anim_time + query.delta_time / 2,query.modified_distance_moved / 1.6)",
			"bones": {
				"bone": {
					"rotation": [0, "math.sin(q.life_time * 432) * 7", 0],
					"position": [0, 0, "-math.abs(math.sin(q.life_time * 432) /7)"]
				},
				"bone5": {
					"rotation": [0, "math.sin(q.life_time * 432 + (14 + query.modified_move_speed / 0.12)) * 8", 0],
					"position": [0, 0, "-math.abs(math.sin(q.life_time * 432 + (14 + query.modified_move_speed / 0.12)) /7)"]
				},
				"bone3": {
					"rotation": [0, "-math.sin(q.life_time * 432 + (14 + query.modified_move_speed / 0.12)) * 4 * 1.25", 0]
				},
				"head": {
					"rotation": [0, "-math.sin(q.life_time * 432 + (14 + query.modified_move_speed / 0.12) * 4) * 6 * 1.25", 0],
					"position": [0, 0, "math.abs(math.sin(q.life_time * 432 + (14 + query.modified_move_speed / 0.12) * 4) /7)"]
				},
				"bone2": {
					"rotation": [0, "math.sin(q.life_time * 432) * 7", 0],
					"position": [0, 0, "-math.abs(math.sin(q.life_time * 432) /7)"]
				},
				"bone6": {
					"rotation": [0, "math.sin(q.life_time * 432 - (14 + query.modified_move_speed / 0.12)) * 7", 0],
					"position": [0, 0, "-math.abs(math.sin(q.life_time * 432 - (14 + query.modified_move_speed / 0.12)) /7)"]
				},
				"bone4": {
					"rotation": [0, "math.sin(q.life_time * 432 - (14 + query.modified_move_speed / 0.12) * 2) * 7", 0],
					"position": [0, 0, "-math.abs(math.sin(q.life_time * 432 - 2 * (14 + query.modified_move_speed / 0.12)) /7)"]
				},
				"bone7": {
					"rotation": [0, "math.sin(q.life_time * 432 - (14 + query.modified_move_speed / 0.12) * 4) * 7", 0],
					"position": [0, 0, "-math.abs(math.sin(q.life_time * 432 - 4 * (14 + query.modified_move_speed / 0.12)) /7)"]
				},
				"body": {
					"rotation": ["query.is_sheared ? q.target_x_rotation / 16 : query.target_x_rotation / 2", 0, 0]
				}
			}
		},
		"animation.billelectriceel.die": {
			"loop": "hold_on_last_frame",
			"animation_length": 1,
			"bones": {
				"body": {
					"rotation": [0, 0, "query.anim_time * 180"],
					"scale": "1 - math.pow(q.anim_time,q.anim_time+1)"
				}
			}
		},
		"animation.billelectriceel.turn": {
			"loop": true,
			"bones": {
				"body": {
					"rotation": [0, 0, 22.5]
				},
				"bone3": {
					"rotation": [0, -7.5, 0]
				},
				"head": {
					"rotation": [0, -10, 0],
					"position": [0, 0, 0.2]
				},
				"bone5": {
					"rotation": [0, 12.5, 0],
					"position": [0, 0, -0.2]
				},
				"bone": {
					"rotation": [0, 12.5, 0],
					"position": [0, 0, -0.2]
				},
				"bone2": {
					"rotation": [0, 12.5, 0],
					"position": [0, 0, -0.2]
				},
				"bone6": {
					"rotation": [0, 12.5, 0],
					"position": [0, 0, -0.2]
				},
				"bone4": {
					"rotation": [0, 12.5, 0],
					"position": [0, 0, -0.2]
				},
				"bone7": {
					"rotation": [0, 10, 0],
					"position": [0, 0, -0.2]
				}
			}
		},
		"animation.billelectriceel.flop": {
			"loop": true,
			"bones": {
				"body": {
					"rotation": [0, 0, "query.life_time * 180"],
					"position": [0, 0, 0]
				},
				"bone": {
					"rotation": [0, "math.sin(q.life_time * 720) * 7 * 1.25", 0]
				},
				"bone5": {
					"rotation": [0, "math.sin(q.life_time * 720 + (14 + query.modified_move_speed / 0.12)) * 8 * 1.25", 0]
				},
				"bone3": {
					"rotation": [0, "-math.sin(q.life_time * 720 + (14 + query.modified_move_speed / 0.12)) * 4 * 1.25", 0]
				},
				"head": {
					"rotation": [0, "-math.sin(q.life_time * 720 + (14 + query.modified_move_speed / 0.12) * 4) * 6 * 1.25", 0]
				},
				"bone2": {
					"rotation": [0, "math.sin(q.life_time * 720) * 7 * 1.25", 0]
				},
				"bone6": {
					"rotation": [0, "math.sin(q.life_time * 720 - (14 + query.modified_move_speed / 0.12)) * 7 * 1.25", 0]
				},
				"bone4": {
					"rotation": [0, "math.sin(q.life_time * 720 - (14 + query.modified_move_speed / 0.12) * 2) * 7 * 1.25", 0]
				},
				"bone7": {
					"rotation": [0, "math.sin(q.life_time * 720 - (14 + query.modified_move_speed / 0.12) * 4) * 7 * 1.25", 0]
				}
			}
		},
		"animation.billelectriceel.sleep": {
			"loop": true,
			"animation_length": 0.5,
			"loop_delay": "math.random(0.7,1.3)",
			"particle_effects": {
				"0.0": {
					"effect": "zee",
					"locator": "zee"
				}
			}
		}
	}
}