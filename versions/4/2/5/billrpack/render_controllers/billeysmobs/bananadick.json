{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.bananaduck": {
      "arrays": {
        "textures": {
          "Array.skins": [
            "Texture.default",
            "Texture.second",
            "Texture.golden",
            "Texture.green",
            "Texture.purple",
            "Texture.white",
            "Texture.inverted"
          ]
        }
      },
      "is_hurt_color": {
          "r": "!query.is_illager_captain",
          "g": "0",
          "b": "0",
          "a": "0.5 + query.is_illager_captain / 3"
      },
      "geometry": "Geometry.default",
      "materials": [ { "*": "Material.default" } ],
      "textures": [ "Array.skins[query.variant]" ],
      //4556616817791844, 5/2025, 216
      //have fun
      "overlay_color": {
          "r": "query.variant == 2 ? math.sin(query.life_time * 250 + query.variant * 234) : math.sin(query.life_time * 200 + query.variant * 234)",
          "g": "query.variant == 2 ? math.sin(query.life_time * 200 + query.variant * 234 + 120) : math.sin(query.life_time * 200 + query.variant * 234 + 120)",
          "b": "query.variant == 2 ? math.sin(query.life_time * 150 + query.variant * 234 + 240) : math.sin(query.life_time * 200 + query.variant * 234 + 240)",
          "a": "query.is_illager_captain ? 0.4 : 0.0"
      }
    }
  }
}
