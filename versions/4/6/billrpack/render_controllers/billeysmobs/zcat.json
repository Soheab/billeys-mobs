{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.zcat": {
      "arrays": {
        "textures": {
          "Array.skins": [
            "Texture.wild",
            "Texture.tamed"
          ]
        }
      },
      "geometry": "Geometry.default",
      "materials": [ { "*": "Material.default" } ],
      "textures": [ "Array.skins[q.is_tamed]" ],
      "part_visibility": [ { "tail3": "q.is_sheared || q.is_interested" },
      { "tail1": "!q.is_sheared && !q.is_interested" },{ "tail4": "q.is_sheared || q.is_interested" },
      { "tail2": "!q.is_sheared && !q.is_interested" },{ "tail5": "q.is_sheared || q.is_interested" },
      { "tail6": "q.is_sheared || q.is_interested" } ],
      "overlay_color": {
          "r": "math.sin(q.life_time * 200 + q.variant * 234)",
          "g": "math.sin(q.life_time * 200 + q.variant * 234 + 120)",
          "b": "math.sin(q.life_time * 200 + q.variant * 234 + 240)",
          "a": "q.is_illager_captain ? 0.4 : 0.0"
      },
      "is_hurt_color": {
          "r": "!q.is_illager_captain",
          "g": "0",
          "b": "0",
          "a": "0.5 + q.is_illager_captain / 3"
      }
    }
  }
}
