                    },
                    "check_beta_apis",
                    {
                        "rainbow_effects": "q.is_illager_captain"
                    },
                    {
                        "stand_up_when_hit": "q.is_tamed"
                    },
                    {
                        "info": "!q.is_tamed"
                    },
                    {
                        "fix": "q.variant == 2 && !q.is_baby"
                    },
                    "gold",
                    "level_when_shooting",
                    {
                        "find_stuff": "q.is_tamed && !q.is_sleeping && !q.is_sitting"
                    },
                    {
                        "find_good_stuff": "(q.property('billey:level') >= 5)&&q.is_tamed &&!q.is_sleeping&&!q.is_sitting"
                    },
                    "tame",
                    "slowfall",
                    "can_sleep"
                ]
