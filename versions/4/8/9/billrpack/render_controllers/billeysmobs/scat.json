{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.scat": {
      "arrays": {
        "textures": {
          "Array.skins": [
            "Texture.default",
            "Texture.default",
            "Texture.default",
            "Texture.default",
            "Texture.default",
            "Texture.default",
