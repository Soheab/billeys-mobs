					},
					{
						"move": "q.modified_move_speed * 1.5"
					}
				]
			},
			"enable_attachables": true,
      "hide_armor": false,
      "render_controllers": [
				"controller.render.billeys_mobs.orangepenguin"
				//in the betta fish file
			],
			"spawn_egg": {
				"overlay_color": "#118101",
				"base_color": "#D16800"
			}
