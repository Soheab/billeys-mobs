{
    "format_version": "1.8.0",
    "render_controllers": {
        "controller.render.billeys_mobs.terraphin": {
            "geometry": "Geometry.default",
            "arrays": {
                "textures": {
                    "Array.base": [
                        "Texture.zero",
                        "Texture.one",
                        "Texture.two",
                        "Texture.three"
                    ],
                    "Array.patterns": [
                        "Texture.zeroa",
                        "Texture.onea",
                        "Texture.twoa",
                        "Texture.threea",
                        "Texture.zerob",
                        "Texture.oneb",
                        "Texture.twob",
                        "Texture.threeb",
                        "Texture.zeroc",
                        "Texture.onec",
                        "Texture.twoc",
                        "Texture.threec",
                        "Texture.zerod",
                        "Texture.oned",
                        "Texture.twod",
                        "Texture.threed"
                    ]
                }
            },
            "materials": [
                {
                    "*": "Material.default"
                }
            ],
            "part_visibility": [
                {
                    "back_fin": "q.variant"                    
                },
                {
                    "tail_fin": "q.variant"                    
                }
                //white ones shouldnt have a tail fluke and a top fin
                //since they spawn in cold biomes and it would make them lose heat
            ],
            "textures": [
                "q.get_name == 'lol' ? Array.base[q.life_time] : Array.base[q.variant]",
                "q.get_name == 'lol' ? Array.patterns[q.life_time*1.33] : Array.patterns[q.mark_variant]"
            ],
            "overlay_color": {
                "r": "math.sin(q.life_time * 200)",
                "g": "math.sin(q.life_time * 200 + 120)",
                "b": "math.sin(q.life_time * 200 + 240)",
                "a": "q.is_illager_captain ? 0.4 : 0.0"
            },
            "is_hurt_color": {
                "r": "!q.is_illager_captain",
                "g": "0",
                "b": "0",
                "a": "0.5 + q.is_illager_captain / 3"
            }
        },
